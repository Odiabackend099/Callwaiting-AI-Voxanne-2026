{
  "name": "roxan-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:all": "concurrently \"npm run dev\" \"cd backend && npm run dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test:frontend": "vitest",
    "test:frontend:watch": "vitest --watch",
    "test:frontend:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:accessibility": "playwright test --grep 'Accessibility'",
    "test:e2e:shortcuts": "playwright test --grep 'Keyboard'",
    "test:e2e:edge-cases": "playwright test --grep 'Edge Case'",
    "test:e2e:ui": "playwright show-report",
    "test:verify-grade": "npm run test:e2e",
    "mock:server": "ts-node tests/mocks/mock-server.ts",
    "test:nuclear": "playwright test tests/telephony-nuclear.test.ts",
    "test:nuclear:ui": "playwright test tests/telephony-nuclear.test.ts --ui",
    "test:nuclear:debug": "playwright test tests/telephony-nuclear.test.ts --debug",
    "test:all": "npm run test:frontend && npm run test:e2e && npm run test:nuclear",
    "generate-icons": "node scripts/generate-pwa-icons.mjs",
    "generate:pwa-icons": "node scripts/generate-pwa-icons.js",
    "generate:pwa-screenshots": "node scripts/generate-pwa-screenshots.js",
    "generate:pwa-all": "npm run generate:pwa-icons && npm run generate:pwa-screenshots",
    "validate:pwa": "./scripts/validate-pwa-assets.sh",
    "dev:pwa": "NEXT_PUBLIC_ENABLE_PWA=true npm run dev"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@studio-freight/react-lenis": "^0.0.47",
    "@supabase/auth-helpers-nextjs": "^0.15.0",
    "@supabase/supabase-js": "^2.39.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.29.2",
    "groq-sdk": "^0.37.0",
    "lenis": "^1.3.15",
    "libphonenumber-js": "^1.12.36",
    "lucide-react": "^0.563.0",
    "next": "14.2.14",
    "next-themes": "^0.2.1",
    "nodemailer": "^6.9.9",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "resend": "^3.2.0",
    "sonner": "^2.0.7",
    "swr": "^2.3.8",
    "tailwind-merge": "^3.4.0",
    "uuid": "^9.0.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.17",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.17",
    "axe-core": "^4.11.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "eslint": "^9",
    "eslint-config-next": "14.2.14",
    "express": "^4.18.2",
    "happy-dom": "^20.3.0",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "postcss": "^8.4.35",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
