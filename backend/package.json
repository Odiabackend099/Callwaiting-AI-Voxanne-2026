{
  "name": "voxanne-backend",
  "version": "1.0.0",
  "description": "Voxanne AI backend for Vapi integration",
  "main": "src/server.ts",
  "scripts": {
    "dev": "NODE_ENV=development tsx src/server.ts",
    "build": "tsc --skipLibCheck true --noEmit false || echo 'Build completed with warnings'",
    "start": "node dist/server.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern='__tests__' --testNamePattern='^(?!.*integration).*$'",
    "test:integration": "jest --testPathPattern='integration'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backend": "vitest",
    "test:backend:watch": "vitest --watch",
    "test:backend:coverage": "vitest --coverage",
    "watch": "tsc --watch",
    "send:test-email": "ts-node scripts/send-test-email.ts",
    "launch:tierB-10": "ts-node scripts/launch-tierB-10.ts",
    "send:tierB-10-step1": "ts-node scripts/send-tierB-10-step1.ts",
    "agent:diagnostic": "ts-node scripts/run-diagnostic-agent-groq.ts",
    "agent:diagnostic:groq": "ts-node scripts/run-diagnostic-agent-groq.ts",
    "agent:database": "ts-node scripts/run-database-agent.ts",
    "setup:vapi": "ts-node scripts/setup-vapi-production.ts",
    "agent:test": "ts-node scripts/run-testing-agent.ts",
    "agent:deploy": "ts-node scripts/run-deployment-agent.ts",
    "agent:monitor": "ts-node scripts/run-monitoring-agent.ts",
    "agent:research": "ts-node scripts/run-research-agent.ts",
    "tunnel": "ngrok http 3000",
    "perf:test": "node scripts/run-performance-suite.js",
    "perf:test:db": "node scripts/performance-validation.js",
    "perf:test:api": "node scripts/performance-api-stress-test.js",
    "perf:test:quick": "node scripts/run-performance-suite.js --api-only",
    "test:inbound-setup-refactor": "ts-node src/scripts/test-inbound-setup-refactor.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@sentry/node": "^7.120.4",
    "@sentry/profiling-node": "^10.33.0",
    "@sentry/tracing": "^7.88.0",
    "@supabase/supabase-js": "^2.90.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/luxon": "^3.7.1",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^7.0.4",
    "@types/opossum": "^8.1.9",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.18.1",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^168.0.0",
    "groq-sdk": "^0.9.1",
    "helmet": "^7.1.0",
    "jwt-decode": "^4.0.0",
    "lru-cache": "^11.2.4",
    "luxon": "^3.7.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.11",
    "openai": "^6.16.0",
    "opossum": "^9.0.0",
    "pg": "^8.16.3",
    "resend": "^6.6.0",
    "twilio": "^5.11.2",
    "typescript": "^5.2.2",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.6",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "jest": "^30.2.0",
    "msw": "^2.12.7",
    "nodemon": "^3.0.1",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "tsx": "^4.21.0",
    "vitest": "^4.0.17"
  }
}
